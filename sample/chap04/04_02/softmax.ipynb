   "source": [
    "# NumPyのインポート\n",
    "import numpy as np\n",
    "\n",
    "# ソフトマックス関数を定義\n",
    "def softmax(x):\n",
    "    y = np.array(np.exp(x) / np.sum(np.exp(x)))\n",
    "    return y"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[3.35308764e-04 9.99541338e-01 1.23353201e-04]\n",
      "0.9999999999999999\n"
     ]
    }
   ],
   "source": [
    "x = np.array([2, 10, 1]) # 成分が3の行ベクトル\n",
    "output =softmax(x)       # ソフトマックス関数の出力を取得\n",
    "print(output)\n",
    "print(sum(output))"
   ]
