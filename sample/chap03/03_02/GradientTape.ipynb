   "source": [
    "import tensorflow as tf\n",
    "\n",
    "x = tf.Variable(3.0)           # 変数を作成\n",
    "with tf.GradientTape() as g:  # with内部の計算を記録\n",
    "    g.watch(x)                 # xを記録(省略可)\n",
    "    y = x * x                  # 微分する式\n",
    "\n",
    "# x = 3のとき、 x * xの傾きを求める\n",
    "# numpy()は出力を見やすくするためのもの\n",
    "g.gradient(y, x).numpy()"
   ]
